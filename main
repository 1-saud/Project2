

public static void main(String[] args) {
    Route r1 = new Route("start1", "end1", 100);
    Route r2 = new Route("start2", "end2", 200);

    Car c1 = new Car("c1", r1, 3);
    Car c2 = new Car("c2", r2, 0);

    Passenger[] ps = new Passenger[2];
    ps[0] = new SubPassenger("ali", 1);
    ps[1] = new NonSubPassenger("Mo", 2, true);

    ps[0].reserveCar(c1);
    ps[1].reserveCar(c2);

    for (int i = 0; i < ps.length; i++) {
        ps[i].showInfo();
    }
}
